@{
    ViewData["Title"] = "Home Page";
}
<script src="https://kit.fontawesome.com/ced37cd46d.js" crossorigin="anonymous"></script>

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="5000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/Contact/Sea.jpg" alt="" class="img-responsive imgC" />
            <div class="carousel-caption" role="option">
                <p class="text-center" style="font-size: 48px">
                    The Beach arguably the best place in the word!
                     <a class="btn btn-primary" style="font-size:28px" asp-controller="Home" asp-action="Wheel">
                        Go test you luck under the sun!<i style="color:coral;" class="fas fa-trophy"></i>
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/Contact/bluesea.jpg" alt="" class="img-responsive " />
            <div class="carousel-caption" role="option">
                <p class="text-center" style="font-size:48px">
                    <a style="color:coral" class="fas fa-umbrella-beach"></a>
                    <a style="color:coral" class="fas fa-volleyball-ball"></a>
                    <a style="color:coral" class="fas fa-fish"></a>
                    <a style="color:coral" class="fas fa-sun"></a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/Contact/mareBack.jpg" alt="" class="img-responsive " />
            <div class="carousel-caption" role="option">
                <p style="font-size: 48px">
                    By scrolling down you'll see our email and our location if you want to contact us!
                </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

<div class=" text-center">
    <address>
        One Microsoft Way<br />
        Redmond, WA 98052-6399<br />
        <abbr title="Phone">P:</abbr>
        425.555.0100
    </address>

    <address>
        <strong>Support:</strong> <a href="mailto:Support@example.com">Support@example.com</a><br />
        <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
    </address>
</div>
<script>
    document.body.style.backgroundColor = "aquamarine";
</script>