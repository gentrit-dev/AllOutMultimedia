@{
    Layout = "_Layout1";
}
<div class="container1">
    <ul id="scene">
        <li class="layer " data-depth=".1">
            <img class="img-responsive" src="~/images/Start/text2.png" />
        </li>
        <li class="layer" data-depth="1">
            <img class="img-responsive" src="~/images/Start/planet1.png" />
        </li>
        <li class="layer " data-depth="-1">
            <img class="img-responsive" src="~/images/Start/planet2.png" />
        </li>
        <li class="layer " data-depth="2">
            <img class="img-responsive" src="~/images/Start/astronaut.png" />
        </li>
        <li class="layer " data-depth="0.5">
            <img class="img-responsive" src="~/images/Start/rocket.png" />
        </li>
    </ul>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/parallax/3.1.0/parallax.js"></script>
<script>
    var scene = document.getElementById('scene');
    var parallax = new Parallax(scene);
</script>

